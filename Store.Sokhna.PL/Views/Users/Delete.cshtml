@model Users
@{
    ViewData["Title"] = "Delete";
    var Role = TempData["Role"] as string;
}
<h1>حذف موظف</h1>
<br />
<form asp-action="Delete">
    <div class="form-group">
        <label asp-for="SSN" class="col-form-label">الرقم القومي</label>
        <input readonly asp-for="SSN" class="form-control" />
        <span asp-validation-for="SSN" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FullName" class="col-form-label">الاسم</label>
        <input readonly asp-for="FullName" class="form-control" />
        <span asp-validation-for="FullName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Job" class="col-form-label">الوظيفه</label>
        <input readonly asp-for="Job" class="form-control" />
        <span asp-validation-for="Job" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Phone" class="col-form-label">رقم الهاتف</label>
        <input readonly asp-for="Phone" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DateofEmployed" class="col-form-label">تاريخ التوظيف</label>
        <input readonly asp-for="DateofEmployed" class="form-control" />
        <span asp-validation-for="DateofEmployed" class="text-danger"></span>
    </div>
    <br />
    <input type="submit" value="حذف" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-info">عوده الي القائمه</a>
</form>

